{
	"version": 1,
	"base_image": "codeocean/mambaforge3:23.1.0-4-python3.10.12-ubuntu22.04",
	"args": [
		"AWS_ACCESS_KEY_ID",
		"AWS_DEFAULT_REGION",
		"AWS_SECRET_ACCESS_KEY"
	],
	"options": {
		"registry_host_arg": true,
		"git_ask_pass": true
	},
	"installers": {
		"apt": {
			"packages": [
				{
					"name": "build-essential",
					"version": "12.9ubuntu3"
				}
			],
			"install_software_properties_common": true
		},
		"pip3": {
			"packages": [
				{
					"name": "git+https://github.com/AllenInstitute/dynamic_routing_analysis#egg=dynamic-routing-analysis",
					"version": "7a41eadcd7974f4bb79083134e6106e4904f1217"
				},
				{
					"name": "matplotlib",
					"version": "3.9.2"
				},
				{
					"name": "pandas[all]",
					"version": "2.2.3"
				},
				{
					"name": "pynwb",
					"version": "2.8.2"
				},
				{
					"name": "scikit-learn",
					"version": "1.5.2"
				},
				{
					"name": "zarr",
					"version": "2.18.0"
				}
			],
			"options": {},
			"pre_install_options": {}
		}
	}
}